@model IEnumerable<MyFile>
@{
    ViewData["Title"] = "My Files";

}


<h1>My Files</h1>

@if(@Model.Count() > 0)
{
    

<table>
    <tr>
        <th>File Name</th>
        <th>File Size</th>
        <th>Uploaded Date</th>
        <th> </th>

    </tr>

    @foreach (MyFile myFile in @Model)
    {
        <tr>
            <th>@myFile.FileName</th>
            <th>@myFile.FileSize</th>
            <th>@myFile.UploadDate</th>
            <th><a asp-controller="Document" asp-action="Manage" asp-route="@myFile.Id" >Manege</a></th>

        </tr>


    }


</table>

}