@model IEnumerable<MyFile>
@{
    ViewData["Title"] = "My Files";
}


<h1>My Files</h1>

@if(@Model.Count() > 0)
{
    @foreach (MyFile myFile in @Model)
    {

    <div class="my-file-container">
        <p class="file-name">@myFile.FileName</p>
        <p class="">@myFile.FileSize</p>
        <p class=""> @myFile.UploadDate</p>
        <a asp-controller="DocumentAccess" asp-action="Index" asp-route-fileId=@myFile.DocumentId>manage</a>

    </div>


    }

}

